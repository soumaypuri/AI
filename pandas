import pandas as pd
dates = pd.date_range(start='2024-04-01', periods=5, freq='D')  #
temperature = [22, 21, 19, 23, 25]
df = pd.DataFrame({
    'Date': dates,
    'Temperature (Â°C)': temperature
})
df.set_index('Date', inplace=True)
print("Weather Data:")
print(df)

# Filter data for specific date
print("\nTemperature on 2024-04-03:")
print(df.loc['2024-04-03'])
